      <function name="ua_string">
        <set name="single">
          <item name="value">HTC_M8</item>
        </set>
      </function>
      <function name="ua_string_mobile">
        <set name="single" />
      </function>
      <function name="ua_string_desktop">
        <set name="single" />
      </function>
      <function name="ua_string_short_desktop">
        <set name="single" />
      </function>
      <!-- UA profile URL -->
      <function name="ua_profile">
        <set name="single" />
      </function>
      <!-- Enable gif animation -->
      <function name="enable_gif_anim">
        <set name="single" />
      </function>
      <!-- Mobile_view -->
      <function name="mobile_view">
        <set name="single">
          <item name="value">true</item>
        </set>
      </function>
      <!-- Default text encoding -->
      <function name="default_text_encoding">
        <set name="single">
          <item name="value">GB2312</item>
        </set>
      </function>
      <!-- Location_Setting -->
      <function name="geolocation">
        <set name="single" />
      </function>
      <!-- Open pages in overview -->
      <function name="overview">
        <set name="single" />
      </function>
      <!-- Max tabs count -->
      <function name="max_tabs">
        <set name="single" />
      </function>
      <!-- full screen mode (show/hide status bar) -->
      <function name="always_show_statusbar">
        <set name="single" />
      </function>
      <!-- Bookmark View Setting -->
      <function name="bookmark_view_mode">
        <set name="single" />
      </function>
      <!-- Most Visit Bookmark View Setting -->
      <function name="mostvisited_view_mode">
        <set name="single" />
      </function>
      <!-- Default Search Engine -->
      <function name="search_engine">
        <set name="single">
          <item name="value">baidu</item>
        </set>
      </function>
      <!-- download_dialog -->
      <function name="download">
        <set name="single" />
      </function>
      <!-- normal_layout -->
      <function name="normal_layout">
        <set name="single" />
      </function>
      <!-- autofit_pages -->
      <function name="autofit_pages">
        <set name="single" />
      </function>
      <!-- mobile_proxy_host -->
      <function name="mobile_proxy_host">
        <set name="single" />
      </function>
      <!-- mobile_proxy_port -->
      <function name="mobile_proxy_port">
        <set name="single" />
      </function>
      <!-- Bookmark enable_plugin-->
      <function name="enable_plugin">
        <set name="single" />
      </function>
    </module>
    <!-- MMS UA string -->
    <module name="MMS">
      <function name="ua_string">
        <set name="single">
          <item name="value">HTC_M8/1.0</item>
        </set>
      </function>
      <function name="ua_profile">
        <set name="single" />
      </function>
    </module>
    <!--  J2ME UA string  -->
    <module name="J2ME">
      <function name="ua_string">
        <set name="single" />
      </function>
    </module>
  </category>
</customization_form>